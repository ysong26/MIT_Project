더하기 빼기 곱하기 나누기 나머지

#include <stdio.h>

int main(void){
	int x,y,sum,a,b,c,d;

	x=7;
	y=3;

	sum=x+y;
	printf("%d + %d = %d \n",x,y,sum);

	a=x-y;
	printf("%d - %d = %d \n",x,y,a);

	b=x*y;
	printf("%d * %d = %d \n",x,y,b);

	c=x/y;
	printf("%d / %d = %d \n",x,y,c);

	d=x%y;
	printf("%d %% %d = %d \n",x,y,d);
}
